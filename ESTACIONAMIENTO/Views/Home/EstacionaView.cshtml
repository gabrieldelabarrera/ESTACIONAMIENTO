@model ESTACIONAMIENTO.Models.Auto

@{
    Layout = null;
}



<!DOCTYPE html>
<!-- saved from url=(0052)http://getbootstrap.com/2.3.2/examples/carousel.html -->
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>Estacionamiento ITGAM</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="~/Content/estilo.css" rel="stylesheet">

    <link href="~/Content/bootstrap.css" rel="stylesheet">
    <link href="~/Content/bootstrap-responsive.css" rel="stylesheet">


    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->
    <!-- Fav and touch icons -->

    <style id="holderjs-style" type="text/css">
        .holderjs-fluid {
            font-size: 16px;
            font-weight: bold;
            text-align: center;
            font-family: sans-serif;
            margin: 0;
        }
    </style>
</head>

<body>

    <!-- NAVBAR
    ================================================== -->
    <div class="navbar-wrapper">
        <!-- Wrap the .navbar in .container to center it within the absolutely positioned parent. -->
        <div class="container">

            <div class="navbar navbar-inverse">
                <div class="navbar-inner">
                    <!-- Responsive Navbar Part 1: Button for triggering responsive navbar (not covered in tutorial). Include responsive CSS to utilize. -->
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="brand" href="http://getbootstrap.com/2.3.2/examples/carousel.html#">ITGAM</a>
                    <!-- Responsive Navbar Part 2: Place all navbar contents you want collapsed withing .navbar-collapse.collapse. -->
                    <div class="nav-collapse collapse">
                        <ul class="nav">
                            <li class="active">
                                <a href="http://getbootstrap.com/2.3.2/examples/carousel.html#">Institucion</a>
                            </li>
                            <li>
                                <a href="http://getbootstrap.com/2.3.2/examples/carousel.html#about">Ayuda</a>
                            </li>
                            <li>
                                <a href="http://getbootstrap.com/2.3.2/examples/carousel.html#contact">Desarrollo</a>
                            </li>
                            <!-- Read about Bootstrap dropdowns at http://twbs.github.com/bootstrap/javascript.html#dropdowns -->
                            <li class="dropdown">
                                <a href="http://getbootstrap.com/2.3.2/examples/carousel.html#" class="dropdown-toggle" data-toggle="dropdown">Desarrollo <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a href="#">Action</a>
                                    </li>
                                    <li>
                                        <a href="#">Another action</a>
                                    </li>
                                    <li>
                                        <a href="http://getbootstrap.com/2.3.2/examples/carousel.html#">Something else here</a>
                                    </li>
                                    <li class="divider"></li>
                                    <li class="nav-header">
                                        Nav header
                                    </li>
                                    <li>
                                        <a href="#">Separated link</a>
                                    </li>
                                    <li>
                                        <a href="#">One more separated link</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div><!-- /.navbar-inner -->
            </div><!-- /.navbar -->

        </div>
        <!-- /.container -->
    </div><!-- /.navbar-wrapper -->
    <!-- Carousel
    ================================================== -->
    <div id="myCarousel" class="carousel slide">
        <div class="carousel-inner">
            <div class="item">
                <img src="~/Content/slide-01.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>ITGAM.</h1>
                        <p class="lead">
                            Calidad.
                        </p>
                        <a class="btn btn-large btn-primary" href="#">Sign up today</a>
                    </div>
                </div>
            </div>
            <div class="item active">
                <img src="~/Content/slide-02.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Sistema Interno.</h1>
                        <p class="lead">
                            Bienvenido al sitema de estacionamiento ITGAM.
                        </p>
                        <a class="btn btn-large btn-primary" href="#">Quienes somos</a>
                    </div>
                </div>
            </div>
            <div class="item">
                <img src="~/Content/slide-03.jpg" alt="">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>INSTITUTO TECNOLOGICO DE GUSTAVO A. MADERO.</h1>
                        <p class="lead">
                            Nos comprometemos con tu seguridad.
                        </p>
                        <a class="btn btn-large btn-primary" href="#">Te puedo ayudar</a>
                    </div>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="http://getbootstrap.com/2.3.2/examples/carousel.html#myCarousel" data-slide="prev">‹</a>
        <a class="right carousel-control" href="http://getbootstrap.com/2.3.2/examples/carousel.html#myCarousel" data-slide="next">›</a>
    </div><!-- /.carousel -->
    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->

    <div class="container marketing">

        <!-- Three columns of text below the carousel -->
        <div class="row">
            <div class="container">


                @{

                    try
                    {


                        <table width="100%" border="1" cellspacing="0" cellpadding="0">
                            <tr>
                                <td width="17%"><strong>Datos</strong></td>
                                <td width="83%"><strong>Información</strong></td>
                            </tr>
                            <tr>
                                <td>Numero de Control</td>
                                <td>@Model.Tarjeton</td>
                            </tr>
                            <tr>
                                <td>Marca</td>
                                <td>@Model.Marca</td>
                            </tr>
                            <tr>
                                <td>Modelo</td>
                                <td>@Model.Modelo</td>
                            </tr>
                            <tr>
                                <td>Color</td>
                                <td>@Model.Color</td>
                            </tr>
                            <tr>
                                <td>Placas</td>
                                <td>@Model.Placas</td>
                            </tr>
                            <tr>
                                <td>Propietario</td>
                                <td>@Model.Propietario</td>
                            </tr>
                            <tr>
                                <td>Imagen</td>
                                <td>
                                    <img src="@Url.Content(Model.PhotoPath)" alt="Imagen 1" width="150" height="150" />
                                </td>
                            </tr>
                        </table>








                    }
                    catch (Exception)
                    {
                        @:<center><p><p><b tarjeton="Error">Vehiculo no reconocido</b></center><p /><p />
        }
                }
                <b>
                    </center>
                    <center>
                        <input type="submit" value="Buscar" onclick="window.location.href = 'http://localhost:7310/'" class="btn btn-large btn-primary" />
                </b>   </center>










            </div>
            <!-- /container -->
        </div><!-- /.row -->
        <!-- START THE FEATURETTES -->
        <!-- Le javascript
        ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="~/Content/jquery.js"></script>
        <script src="~/Content/bootstrap-transition.js"></script>
        <script src="~/Content/bootstrap-alert.js"></script>
        <script src="~/Content/bootstrap-modal.js"></script>
        <script src="~/Content/bootstrap-dropdown.js"></script>
        <script src="~/Content/bootstrap-scrollspy.js"></script>
        <script src="~/Content/bootstrap-tab.js"></script>
        <script src="~/Content/bootstrap-tooltip.js"></script>
        <script src="~/Content/bootstrap-popover.js"></script>
        <script src="~/Content/bootstrap-button.js"></script>
        <script src="~/Content/bootstrap-collapse.js"></script>
        <script src="~/Content/bootstrap-carousel.js"></script>
        <script src="~/Content/bootstrap-typeahead.js"></script>
        <script>
            ! function ($) {
                $(function () {
                    // carousel demo
                    $('#myCarousel').carousel()
                })
            }(window.jQuery)
        </script>
        <script src="./index_files/holder.js"></script>

</body>
</html>


































